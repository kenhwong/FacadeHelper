                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Normal">
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>

                        <Path x:Name="ButtonBack" 
                              Data="M15,0 A15,15 0 1 0 15,30 A15,15 0 1 0 15,0Z" 
                              Fill="Transparent" 
                              Stretch="Fill" 
                              Width="{TemplateBinding Width}" 
                              Height="{TemplateBinding Height}" 
                              HorizontalAlignment="Left" 
                              StrokeThickness="1"/>
                        <Path x:Name="BookShape"
                          Data="{TemplateBinding Tag}"
                          Stretch="Fill" Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" StrokeThickness="0"
                          Fill="{TemplateBinding Foreground}" HorizontalAlignment="Left" />
